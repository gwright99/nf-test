manifest.name = "Fusion debugging"
wave.enabled = true
fusion.enabled = true
process.container = 'quay.io/biocontainers/star-fusion:1.9.1--hdfd78af_1'

profile {
    docker {
        docker {
        enabled = true
        runOptions = '-v $HOME/.aws:/root/.aws'
        envWhitelist = 'AWS_PROFILE'
        }
    }
}